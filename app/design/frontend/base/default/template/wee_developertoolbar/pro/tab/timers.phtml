<?php
$_helper = Mage::helper('wee_developertoolbar');
$timers = $this->getTimerData();

$i = 0;
?>
<table class="timerTable stripped sortable" border="0" cellpadding="0" cellspacing="0">
  <tr>
    <th>Code</th>
    <th>Number</th>
    <th>Class</th>
    <th>Template (Block Id)</th>
    <th>Childs</th>
    <th sort="float">Creating</th>
    <th sort="float">Rendering</th>
