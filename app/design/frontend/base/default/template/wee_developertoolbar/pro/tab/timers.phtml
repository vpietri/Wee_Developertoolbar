    <td><?php echo $blockName . (!empty($blockTimers[5]) ? ' <i>('.$blockTimers[5].')</i>' :''); ?></td>
    <td><?php echo $blockTimers[0]; ?></td>
    <td><?php echo $blockTimers[1]; ?></td>
    <td><?php echo $blockTimers[2]; ?></td>
    <td><?php echo $blockTimers[7]; ?></td>
    <td><?php echo $_helper->formatSqlTime($blockTimers[3]); ?></td>
    <td><?php echo $_helper->formatSqlTime($blockTimers[4]); ?></td>
  </tr>
<?php $i++; ?>
<?php endforeach;?>
</table>